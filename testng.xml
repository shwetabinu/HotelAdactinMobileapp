<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Hotel Adactin Mobile App Regression Test Suite">

	<test name="Login Verification with no user id and password">
		<classes>
			<class name="com.adactin.Mobileapphoteladactin1.tests.BlankLoginVerificationTest" />		
		</classes>
	</test> 
	
	<test name = "Login Verification with invalid user id and password">
		<classes>
			<class name="com.adactin.Mobileapphoteladactin1.tests.InvalidLoginVerificationTest"/>
		</classes>
	</test>
	
	<test name = "Login verification with valid user id and password">
		<classes>
			<class name="com.adactin.Mobileapphoteladactin1.tests.ValidLoginVerificationTest"/>
		</classes>
	</test>
</suite> 

